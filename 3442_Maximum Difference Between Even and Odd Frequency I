class Solution {
public:
    int maxDifference(string s) {
        vector<int>count(26,0);
        for(int i = 0; i < s.length(); i++){
            count[s[i]-'a']++;
        }
        int mini = INT_MAX,maxi = 0;
        for(int i = 0; i < 26; i++){
            cout<<char(i+'a')<<" "<<count[i]<<endl;
            if(count[i] != 0 && count[i]%2 == 0)mini = min(mini,count[i]);
            if(count[i]%2 != 0)maxi = max(maxi,count[i]);
        }

        return maxi-mini;
    }
};
